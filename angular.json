{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-aio-forms": {
      "projectType": "library",
      "root": "projects/ng-aio-forms",
      "sourceRoot": "projects/ng-aio-forms/src",
      "prefix": "ng-aio-forms",
      "styles":["../node_modules/normalize.css/normalize.css"],
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-aio-forms/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-aio-forms/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-aio-forms/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-aio-forms/src/test.ts",
            "tsConfig": "projects/ng-aio-forms/tsconfig.spec.json",
            "karmaConfig": "projects/ng-aio-forms/karma.conf.js"
          }
        }
      }
    },
    "ng-aio-forms-tester": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/ng-aio-forms-tester",
      "sourceRoot": "projects/ng-aio-forms-tester/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ng-aio-forms-tester",
            "index": "projects/ng-aio-forms-tester/src/index.html",
            "main": "projects/ng-aio-forms-tester/src/main.ts",
            "polyfills": "projects/ng-aio-forms-tester/src/polyfills.ts",
            "tsConfig": "projects/ng-aio-forms-tester/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ng-aio-forms-tester/src/favicon.ico",
              "projects/ng-aio-forms-tester/src/assets"
            ],
            "styles": [
              "projects/ng-aio-forms-tester/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "100kb",
                  "maximumError": "200kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/ng-aio-forms-tester/src/environments/environment.ts",
                  "with": "projects/ng-aio-forms-tester/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ng-aio-forms-tester:build:production"
            },
            "development": {
              "browserTarget": "ng-aio-forms-tester:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-aio-forms-tester:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-aio-forms-tester/src/test.ts",
            "polyfills": "projects/ng-aio-forms-tester/src/polyfills.ts",
            "tsConfig": "projects/ng-aio-forms-tester/tsconfig.spec.json",
            "karmaConfig": "projects/ng-aio-forms-tester/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ng-aio-forms-tester/src/favicon.ico",
              "projects/ng-aio-forms-tester/src/assets"
            ],
            "styles": [
              "projects/ng-aio-forms-tester/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "ng-aio-forms-website": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/ng-aio-forms-website",
      "sourceRoot": "projects/ng-aio-forms-website/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ng-aio-forms-website",
            "index": "projects/ng-aio-forms-website/src/index.html",
            "main": "projects/ng-aio-forms-website/src/main.ts",
            "polyfills": "projects/ng-aio-forms-website/src/polyfills.ts",
            "tsConfig": "projects/ng-aio-forms-website/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ng-aio-forms-website/src/favicon.ico",
              "projects/ng-aio-forms-website/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
              "projects/ng-aio-forms-website/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "100kb",
                  "maximumError": "200kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/ng-aio-forms-website/src/environments/environment.ts",
                  "with": "projects/ng-aio-forms-website/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ng-aio-forms-website:build:production"
            },
            "development": {
              "browserTarget": "ng-aio-forms-website:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-aio-forms-website:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-aio-forms-website/src/test.ts",
            "polyfills": "projects/ng-aio-forms-website/src/polyfills.ts",
            "tsConfig": "projects/ng-aio-forms-website/tsconfig.spec.json",
            "karmaConfig": "projects/ng-aio-forms-website/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ng-aio-forms-website/src/favicon.ico",
              "projects/ng-aio-forms-website/src/assets"
            ],
            "styles": [
              "projects/ng-aio-forms-website/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }

  },
  "defaultProject": "ng-aio-forms"
}
